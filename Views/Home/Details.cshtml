@model List<TaskWebApplication.Models.Employee>
@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>
<table class="table table-dark table-bordered table-responsive table-hover">

    <tr>

        <th> Emp_Id</th>
        <th> Name</th>
        <th> Mobile</th>
        <th> Email</th>
        <th> Address</th>
        <th> Salary</th>
        <th> DOJ</th>
        <th> Edit</th>
        <th> Delete</th>



    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Emp_Id</td>
            <td>@item.Name</td>
            <td>@item.Mobile</td>
            <td>@item.Email</td>
            <td>@item.Address</td>
            <td>@item.Salary</td>
            <td>@item.DOJ</td>

            <td><a class="btn btn-warning" asp-action="Edit" asp-controller="Home" asp-route-Id="@item.Emp_Id">Edit </a></td>
            <td><a class="btn btn-danger" asp-action="Delete" asp-controller="Home" asp-route-Id="@item.Emp_Id">Delete </a></td>



        </tr>
    }
</table>